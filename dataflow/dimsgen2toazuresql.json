{
	"name": "dimsgen2toazuresql",
	"properties": {
		"folder": {
			"name": "gen2toazuresqlvalidation"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"name": "teacherAddress"
				},
				{
					"name": "student"
				},
				{
					"name": "studentAdress"
				}
			],
			"sinks": [
				{
					"name": "teacherAdressDim"
				},
				{
					"name": "studentDim"
				},
				{
					"name": "studentAdressDim"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          teacher_address_id as integer,",
				"          teacher_id as integer,",
				"          state as string,",
				"          city as string,",
				"          watermark as date 'dd-MM-yyyy'",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false) ~> teacherAddress",
				"source(output(",
				"          student_id as integer,",
				"          name as string,",
				"          branch as string,",
				"          watermark as date 'dd-MM-yyyy',",
				"          teacher_id as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false) ~> student",
				"source(output(",
				"          student_address_id as integer,",
				"          student_id as integer,",
				"          state as string,",
				"          city as string,",
				"          watermark as date 'yyyy-MM-dd'",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false) ~> studentAdress",
				"teacherAddress sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     format: 'table') ~> teacherAdressDim",
				"student sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     format: 'table') ~> studentDim",
				"studentAdress sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     format: 'table') ~> studentAdressDim"
			]
		}
	}
}